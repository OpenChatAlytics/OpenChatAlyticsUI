{
  "name": "openchatalyticsui",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "typings install",
    "pretest": "webpack --config webpack.config.test.js",
    "test": "nyc ava",
    "watch": "npm-run-all --parallel watch:*",
    "watch:test": "webpack --config webpack.config.test.js --watch",
    "watch:ava": "ava --watch",
    "start": "webpack-dev-server"
  },
  "ava": {
    "files": [
      ".build/test/**/*.js"
    ],
    "require": [
      "jsdom-global/register",
      "source-map-support/register"
    ],
    "concurrency": 4
  },
  "nyc": {
    "exclude": [
      "test",
      "dist/test",
      "node_modules"
    ]
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.6.0",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "glob": "^7.1.1",
    "jsdom": "^9.8.3",
    "jsdom-global": "^2.1.0",
    "lodash": "^4.17.2",
    "node-sass": "^3.11.3",
    "npm-run-all": "^3.1.1",
    "nyc": "^9.0.1",
    "react-addons-test-utils": "^15.4.0",
    "react-bootstrap": "^0.30.6",
    "react-dom": "^15.4.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-loader": "^2.4.0",
    "sass-loader": "^4.0.2",
    "source-map-loader": "^0.1.5",
    "source-map-support": "^0.4.6",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.2.0",
    "tslint": "^4.0.1",
    "typescript": "^2.0.9",
    "typings": "^2.0.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "react": "15.4.0",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-saga": "^0.13.0"
  }
}
