{
  "name": "chatalytics-ui",
  "version": "0.0.0",
  "description": "UI for realtime platform for processing Slack and HipChat messages",
  "main": "index.js",
  "apps" : [{
    "name": "search",
    "script": "server/index.js",
    "watch": true,
    "ignore_watch": ["client", ".gitignore", ".git", "node_modules", "__tests__"]
  }],
  "scripts": {
    "start": "pm2 start package.json --no-daemon",
    "test": "jest",
    "coverage": "jest --coverage",
    "dist": "cross-env NODE_ENV=production webpack -p --progress --config server/webpack.config.js",
    "postinstall": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/psastras/ChatAlyticsUI"
  },
  "engines": {
    "node": "5",
    "npm": "3"
  },
  "keywords": [
    "slack",
    "hipchat",
    "analytics"
  ],
  "author": "Paul Sastrasinh",
  "bugs": {
    "url": "https://github.com/psastras/ChatAlyticsUI/issues"
  },
  "homepage": "https://github.com/psastras/ChatAlyticsUI",
  "dependencies": {
    "alt": "0.18.4",
    "alt-utils": "1.0.0",
    "babel-cli": "6.5.1",
    "babel-polyfill": "6.3.14",
    "babel-runtime": "6.3.19",
    "babel-register": "6.7.2",
    "compression": "1.6.1",
    "cross-env": "1.0.7",
    "ejs": "2.4.1",
    "express": "4.13.4",
    "express-ejs-layouts": "2.0.0",
    "express-ws": "2.0.0-beta",
    "font-awesome": "4.5.0",
    "highcharts": "4.2.3",
    "humanize": "0.0.9",
    "log4js": "0.6.33",
    "marked": "0.3.5",
    "morgan": "1.7.0",
    "measured": "1.1.0",
    "pm2": "1.0.2",
    "react": "0.14.6",
    "react-addons-css-transition-group": "0.14.6",
    "react-addons-pure-render-mixin": "0.14.6",
    "react-dom": "0.14.6",
    "react-highcharts": "8.1.1",
    "react-lazy-load": "3.0.7",
    "react-parallax": "0.4.2",
    "react-router": "2.0.1",
    "react-table": "1.3.0",
    "request": "2.70.0",
    "react-easy-transition": "0.0.2",
    "spinkit": "1.2.5",
    "superagent": "1.8.3",
    "webpack": "1.12.12",
    "ws": "1.1.0",
    "yaml-config-loader": "2.0.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.2.0",
    "autoprefixer-loader": "3.2.0",
    "babel-core": "6.4.5",
    "babel-jest": "6.0.1",
    "babel-loader": "6.2.1",
    "babel-plugin-transform-runtime": "6.4.3",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "coveralls": "2.11.6",
    "css-loader": "0.23.1",
    "file-loader": "0.8.5",
    "jest-cli": "0.10.2",
    "image-webpack-loader": "1.6.3",
    "node-sass": "3.4.2",
    "react-addons-test-utils": "0.14.6",
    "react-hot-loader": "1.3.0",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "webpack-dev-server": "1.14.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/classnames",
      "<rootDir>/node_modules/dom-helpers",
      "<rootDir>/node_modules/invariant",
      "<rootDir>/node_modules/keycode",
      "<rootDir>/node_modules/lodash-compat",
      "<rootDir>/node_modules/uncontrollable",
      "<rootDir>/node_modules/warning",
      "<rootDir>/node_modules/babel-runtime",
      "<rootDir>/node_modules/core-js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testFileExtensions": [
      "test.js",
      "test.jsx"
    ],
    "moduleNameMapper": {
      "^[./a-zA-Z0-9$_-]+\\.(jpg|jpeg|ico|png)$": "__tests__/StaticImageStub.js"
    }
  }
}
