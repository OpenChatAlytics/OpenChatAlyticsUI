{
  "name": "openchatalyticsui",
  "version": "0.0.0",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js --progress",
    "build:test": "webpack --config webpack.config.test.js",
    "postinstall": "typings install && selenium-standalone install",
    "pretest": "npm-run-all build:test",
    "test": "nyc ava",
    "precitest": "npm-run-all build:test",
    "citest": "nyc ava --tap | tap-xunit > $CI_TEST_REPORT_FILE",
    "postcitest": "npm-run-all lint",
    "lint": "tslint --project src/tsconfig.json",
    "preintegrationtest": "webpack --config webpack.config.test.js",
    "integrationtest:test": "ava '.build/integration-test/**/*.js' --verbose -c 1",
    "integrationtest:runner": "node .build/integration-test/_runner.ts",
    "integrationtest": "npm-run-all build integrationtest:runner",
    "watch": "npm-run-all --parallel watch:*",
    "watch:server": "webpack-dev-server",
    "watch:test": "webpack --config webpack.config.test.js --watch",
    "watch:ava": "ava --watch"
  },
  "ava": {
    "files": [
      ".build/test/**/*.js"
    ],
    "require": [
      "jsdom-global/register",
      "source-map-support/register"
    ],
    "concurrency": 4
  },
  "nyc": {
    "include": [
      ".build/src/**/*.js"
    ]
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.24.1",
    "jsdom": "^9.8.3",
    "jsdom-global": "^2.1.0",
    "lodash": "^4.17.2",
    "node-sass": "^3.11.3",
    "npm-run-all": "^3.1.1",
    "nyc": "^9.0.1",
    "react-addons-test-utils": "^15.4.0",
    "react-bootstrap": "^0.30.6",
    "react-dom": "^15.4.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-loader": "^2.4.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "selenium-standalone": "^5.8.0",
    "source-map-loader": "^0.1.5",
    "source-map-support": "^0.4.6",
    "style-loader": "^0.13.1",
    "tap-xunit": "^1.4.0",
    "ts-loader": "^1.2.0",
    "tslint": "^4.0.1",
    "typescript": "^2.0.9",
    "typings": "^2.0.0",
    "webdriverio": "^4.4.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "react": "15.4.0",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-saga": "^0.13.0"
  }
}
